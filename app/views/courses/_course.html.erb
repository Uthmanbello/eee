<div id="<%= dom_id course %>", class='bills-list'>
  <div class='bill-icon text2'>
    <p><%= course.semester %></p>
  </div>

  <div class= 'appoint d-none'>
    <p class= 'grey'><%= course.units %> units</p>
  </div>

  <div class= 'flex-column'>
    <p class= 'text2'><%= course.title %></p>
    <p class= 'time'><%= course.code %></p>
  </div>

  <div class= 'bill-total'>
    <p class= 'grey'><%= course.year %></p>
  </div>

</div>
